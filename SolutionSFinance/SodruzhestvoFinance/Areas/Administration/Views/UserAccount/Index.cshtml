@using Microsoft.AspNetCore.Identity
@using SodruzhestvoFinance.Areas.Administration.Models
@{
    ViewData["Title"] = "Учётные записи";
    Layout = "_Layout";

    List<ApplicationUser> users = ViewBag.Users; // Получаем список пользователей из ViewBag
}
<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Имя пользователя</th>
            
            <!-- Добавьте другие столбцы, которые вам нужны -->
        </tr>
    </thead>
    <tbody>
        @if (users != null && users.Any())
        {
            foreach (var user in users)
            {
                <tr>
                    <td>@user.UserName</td>
                    <!-- Добавьте другие столбцы, которые вам нужны -->
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="2">Пользователи не найдены.</td>
            </tr>
        }
    </tbody>
</table>